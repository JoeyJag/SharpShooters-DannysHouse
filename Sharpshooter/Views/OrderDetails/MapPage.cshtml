@{
    ViewBag.Title = "MapPage";
    Layout = "~/Views/Shared/TempLayout.cshtml";
}

<!DOCTYPE html>
<html>
<head>
    <title>Simple Map</title>
    <script src="https://polyfill.io/v3/polyfill.min.js?features=default"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBDUc0EGf1-Fm6Urj1n3xn4ev-FZz3BZ1s&callback=initMap&libraries=&v=weekly"
            defer></script>
    <style type="text/css">
         /* Always set the map height explicitly to define the size of the div
        * element that contains the map. */
         #map {
             height: 100%;
         }

         /* Optional: Makes the sample page fill the window. */
         html,
         body {
             height: 100%;
             margin: 0;
             padding: 0;
         }
    </style>
    <script>
      let map;

        function initMap()
        {
            map = new google.maps.Map(document.getElementById("map"),
            {
                center: { lat: -29.5764, lng: 31.1126632 },
                zoom: 20,
            });
        }
    </script>
</head>
<body>
    <div id="map"></div>
</body>
</html>
